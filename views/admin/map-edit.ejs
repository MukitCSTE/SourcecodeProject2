                </div>

                <label class="form-label"  for="exampleFormControlSelect1">Set Active Status</label>
                <div class="input-group input-group-dynamic mb-4">
                  
                  <div class="input-group input-group-static mb-4">
               

                    <select class="selectpicker" name="status" value="<%=res.active_status%>"> 
                      <% for(var i=0; i< 2; i++){ %>
                      
                        <% if(res.active_status == i){ %>
                          <option selected><%= res.active_status %></option>
                        <% } else{ %>  
                          <option> <%= i %></option>
                       <% } %>
                       
                    <% } %>
  
                  </select>
                  </div>
                </div>
          

                <p>Change Password:</p>

                    <div>
                      <input type="checkbox" id="checkbox" name="password" onclick="myFunction()">
                      <label for="scales">Do you want to update map user password</label>
                      <div class="input-group input-group-outline mb-4">
                        <input type="text" id="password" class="form-control" placeholder="type new password"  style="display:none"  name="password"  >
                
                      </div>
                      </div>
                   
                    
                
                </div>
                <div class="input-group input-group-dynamic mb-4">
                  <button style="margin-right: 5px;" type="submit" class="btn btn-primary">Update</button>
          
                  <button onclick="location.href='/admin/role'" type="button" class="btn btn-secondary">Cancel</button>
                  <div class="">
                    <% for(var i=0; i< errs.length; i++){ %>
                        <%= errs[i].message %>
                        <br>
                    <% } %>

                </div>
              </div>
              </form>
