            <h6 class="text-white text-capitalize ps-3">Create New Map User</h6>
          </div>
        </div>
        <div class="card-body px-0 pb-2">

          <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
           
            <div class="p-4">
              <form method="POST">
                <div class="input-group input-group-outline mb-4">
                  <label class="form-label">Map User Name</label>
                  <input type="text" class="form-control" name="user_name">
                </div>
                <div class="input-group input-group-outline mb-4">
                  <label class="form-label">Email</label>
                  <input type="text" class="form-control" name="user_email">
                </div>

                <label class="form-label"  for="exampleFormControlSelect1">Set Active Status</label>
                <div class="input-group input-group-dynamic mb-4">
                  
                  <div class="input-group input-group-static mb-4">
                    
                    <select class="selectpicker" id="exampleFormControlSelect1" name="status"> 
                      <option >0</option>
                      <option selected>1</option>
                    
                    </select>
                  </div>
                </div>
                <div class="input-group input-group-outline mb-4">
                  <label class="form-label">Password</label>
                  <input type="text" class="form-control" name="password">
                </div>

             
               
                <div class="input-group input-group-dynamic mb-4">
                <button type="submit" class="btn btn-info">Save</button>

             
                <div class="">
                  <% for(var i=0; i< errs.length; i++){ %>
                      <%= errs[i].message %>
                      <br>
                  <% } %>

              </div>
             
                
              </div>
              </form>
            </div>
          </div>
          
        </div>
      </div>
    </div>


    <div class="row">
